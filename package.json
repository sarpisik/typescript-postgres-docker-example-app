{
  "name": "typescript-postgres-docker-example-app",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/express-validator": "^3.0.0",
    "@types/helmet": "0.0.45",
    "@types/mocha": "^5.2.7",
    "@types/node": "^8.0.29",
    "@types/sinon": "^7.5.1",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.9",
    "mocha": "^6.2.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.1",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "sinon": "^7.5.0",
    "ts-node": "3.3.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.18.1",
    "cors": "^2.8.5",
    "express": "^4.15.4",
    "express-validator": "^6.3.0",
    "helmet": "^3.21.2",
    "pg": "^7.3.0",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.21",
    "winston": "^3.2.1"
  },
  "scripts": {
    "start": "node dist/src/index.js",
    "postinstall": "tsc",
    "dev": "nodemon",
    "test:mocha": "mocha dist/test/**/*.test.js",
    "test:clean": "rimraf dist && tsc",
    "test": "npm run test:clean && nyc --reporter=html npm run test:mocha --exit",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/coveralls/bin/coveralls",
    "codeclimate": "codeclimate-test-reporter < lcov.info",
    "coverage": "npm run coveralls && npm run codeclimate",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\""
  }
}
